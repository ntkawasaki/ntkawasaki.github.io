    }
  }

  <span class="pl-c"><span class="pl-c">#</span> Send to a particular Slack channel with a preset message</span>
  <span class="pl-v">action</span><span class="pl-k">:</span> {
    <span class="pl-v">label</span><span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>Ping Channel<span class="pl-pds">"</span></span>
    <span class="pl-v">url</span><span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>https://example.com/data_actions<span class="pl-pds">"</span></span>
    <span class="pl-v">param</span><span class="pl-k">:</span> {
      <span class="pl-v">name</span><span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>message<span class="pl-pds">"</span></span>
      <span class="pl-v">value</span><span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>:signal_strength: I sent a value from Slack: {{rendered_value}}<span class="pl-pds">"</span></span>
    }
    <span class="pl-v">param</span><span class="pl-k">:</span> {
      <span class="pl-v">name</span><span class="pl-k">:</span> <span class="pl-s"><span class="pl-pds">"</span>channel<span class="pl-pds">"</span></span>
